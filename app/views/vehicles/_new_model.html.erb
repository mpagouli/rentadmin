<div id="model_new_dialog" title="New Model">
	<h2>New Model</h1>
	<div class="row">
	  <div class="span5">
	    <%= form_tag "/models" do %>
	      <%= render 'makeform', make: make %>
	      <div class="center-button">
			<%= submit_tag "Insert Model", class: "btn btn-large btn-primary" %>
		  </div>
	    <% end %>
	  </div>
	</div>
</div>