<div id="model_new_dialog" title="New Model">
	<div class="row">
	  <div class="span3">
	    <%= form_tag "/models" do %>
	      <%= render 'modelform', model: model %>
	      <div class="center-button">
			<%= submit_tag "Insert Model", class: "btn btn-large btn-primary" %>
		  </div>
	    <% end %>
	  </div>
	</div>
</div>